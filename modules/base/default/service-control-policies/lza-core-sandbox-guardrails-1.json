{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "GRNETSEC1",
      "Effect": "Deny",
      "Action": [
        "ec2:DeleteTags"
      ],
      "Resource": "*",
      "Condition": {
        "StringEquals": {
          "ec2:ResourceTag/Accelerator": "${ACCELERATOR_PREFIX}"
        },
        "ArnNotLike": {
          "aws:PrincipalARN": [
            "arn:${PARTITION}:iam::*:role/${ACCELERATOR_PREFIX}*",
            "arn:${PARTITION}:iam::*:role/${MANAGEMENT_ACCOUNT_ACCESS_ROLE}",
            "arn:${PARTITION}:iam::*:role/cdk-accel*"
          ]
        }
      }
    },
    {
      "Sid": "GRNETSEC2",
      "Effect": "Deny",
      "Action": [
        "ec2:AcceptTransitGateway*",
        "ec2:AcceptVpc*",
        "ec2:AllocateAddress",
        "ec2:AssociateAddress",
        "ec2:AttachInternetGateway",
        "ec2:AttachVpnGateway",
        "ec2:CreateEgressOnlyInternetGateway",
        "ec2:CreateInternetGateway",
        "ec2:CreateIpam*",
        "ec2:CreateNatGateway",
        "ec2:CreateRoute*",
        "ec2:CreateSubnet",
        "ec2:CreateTransitGateway*",
        "ec2:CreateVpc",
        "ec2:CreateVpcEndpoint",
        "ec2:CreateVpcPeeringConnection",
        "ec2:CreateVpn*",
        "ec2:DeleteIpam",
        "ec2:DeleteRoute*",
        "ec2:DeleteSubnet",
        "ec2:DeleteTransitGateway*",
        "ec2:DeleteVpc",
        "ec2:ModifyIpam*",
        "ec2:ModifyTransitGateway*"
      ],
      "Resource": "*",
      "Condition": {
        "StringEquals": {
          "ec2:ResourceTag/Accelerator": "${ACCELERATOR_PREFIX}"
        },
        "ArnNotLike": {
          "aws:PrincipalARN": [
            "arn:${PARTITION}:iam::*:role/${ACCELERATOR_PREFIX}*",
            "arn:${PARTITION}:iam::*:role/${MANAGEMENT_ACCOUNT_ACCESS_ROLE}",
            "arn:${PARTITION}:iam::*:role/cdk-accel*"
          ]
        }
      }
    },
    {
      "Sid": "GREFS",
      "Effect": "Deny",
      "Action": "elasticfilesystem:CreateFileSystem",
      "Resource": "*",
      "Condition": {
        "Bool": {
          "elasticfilesystem:Encrypted": "false"
        }
      }
    },
    {
      "Sid": "GRRDS1",
      "Effect": "Deny",
      "Action": "rds:CreateDBInstance",
      "Resource": "arn:${PARTITION}:rds:*:*:db:*",
      "Condition": {
        "StringNotLike": {
          "rds:DatabaseEngine": [
            "aurora*"
          ]
        },
        "Bool": {
          "rds:StorageEncrypted": "false"
        }
      }
    },
    {
      "Sid": "GRRDS2",
      "Effect": "Deny",
      "Action": "rds:CreateDBCluster",
      "Resource": "*",
      "Condition": {
        "StringLike": {
          "rds:DatabaseEngine": [
            "aurora*"
          ]
        },
        "Bool": {
          "rds:StorageEncrypted": "false"
        }
      }
    }
  ]
}